<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.v0.hierarchical-filter" extension-version="1.0.0">
    <default-locale>es_ES</default-locale>
    <name resource-id="name"/>
    <description>Filtro jerárquico para datos con estructura usuario-líder</description>
    <author name="v0" email="support@v0.dev" organization="Vercel" website="https://v0.dev"/>
    <min-api-version>1.0</min-api-version>
    <source-location>
      <!-- URL de Vercel actualizada -->
      <url>https://filterjerarquia.vercel.app</url>
    </source-location>
    <icon></icon>
    <permissions>
      <permission>full data</permission>
    </permissions>
    <context-menu>
      <configure-context-menu-item/>
    </context-menu>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="es_ES">Filtro Jerárquico</text>
      <text locale="en_US">Hierarchical Filter</text>
    </resource>
  </resources>
</manifest>
